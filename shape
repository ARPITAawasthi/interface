Interface Shape
{
void circle(int);
void rectangle(int,int);
}

class Area implements Shape
{
public void circle(int r)
{
int a;
a= 3.14*r*r;
System.out.println("Area of circle is="+a);
}
public void rectangle(int l,int b)
{
int c;
c=l*b;
System.out.println("Area of a rectanglr is="+c);
}
}
class Perimeter implements Shape
{
public void circle(int r)
{
int p;
p=2*3.14*r;
System.out.println("Perimeter of a circle is="+p);
}
public void rectangle(int l,int b)
{
 int s;
 s=2*l*b;
 System.out.println("Perimeter of a rectangle is="+s);
 }
 }
 class abc
 {
 public static void main(String[] args)
 {
 Shape i=new Area();
 i.circle(5);
 i.rectangle(3,4);
 Shape it1=new Perimeter();
 it1.circle(7);
 it1.rectangle(4,5);
 }
 }
 
